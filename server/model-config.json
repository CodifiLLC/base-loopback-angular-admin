{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "db",
    "public": false
  },
  "AccessToken": {
    "dataSource": "persistent",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "Email": {
    "dataSource": "emailDs"
  },
  "RoleMapping": {
    "dataSource": "persistent",
    "public": true
  },
  "Role": {
    "dataSource": "db",
    "public": true
  },
  "CustomUser": {
    "dataSource": "persistent",
    "public": true,
    "options": {
      "emailVerificationRequired": true
    }
  }
}
