{
  "name": "base-loopback-angular-admin",
  "version": "1.0.0",
  "main": "server/server.js",
  "angular-cli": {},
  "scripts": {
    "build:sdk": "./node_modules/.bin/lb-sdk server/server.js client/src/app/shared/sdk -i enabled",
    "lint": "eslint . && tslint \"client/src/**/*.ts\"",
    "start": "ng build && node .",
    "dev": "ng serve & node-dev .",
    "test": "ng test --single-run && ./node_modules/jasmine/bin/jasmine.js JASMINE_CONFIG_PATH=jasmine.json",
    "posttest.bak": "npm run lint && nsp check",
    "pree2e": "webdriver-manager update",
    "e2e": "(cd client && protractor)"
  },
  "dependencies": {
    "@angular/common": "2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/forms": "2.2.1",
    "@angular/http": "2.2.1",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/router": "3.2.1",
    "compression": "^1.0.3",
    "core-js": "^2.4.1",
    "cors": "^2.5.2",
    "helmet": "^1.3.0",
    "loopback": "^3.0.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^2.4.0",
    "loopback-connector-mysql": "^2.4.0",
    "loopback-context": "^1.0.0",
    "rxjs": "5.0.0-beta.12",
    "serve-favicon": "^2.0.1",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.7.2",
    "strong-error-handler": "^1.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.2.1",
    "@mean-expert/loopback-sdk-builder": "^2.1.0-rc.2",
    "@types/jasmine": "^2.5.38",
    "@types/node": "^6.0.42",
    "@types/socket.io-client": "^1.4.29",
    "angular-cli": "1.0.0-beta.21",
    "codelyzer": "~1.0.0-beta.3",
    "enhanced-resolve": "^2.3.0",
    "eslint": "^2.13.1",
    "eslint-config-loopback": "^4.0.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "node-dev": "^3.1.3",
    "nsp": "^2.1.0",
    "protractor": "4.0.9",
    "ts-node": "1.2.1",
    "tslint": "3.13.0",
    "typescript": "~2.0.3",
    "webdriver-manager": "10.2.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CodifiLLC/base-loopback-angular-admin.git"
  },
  "license": "MIT",
  "description": "A base project (Angular 2, Loopback 3, and Socket.io) for building an application with user account and admin functions"
}
