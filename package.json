{
  "name": "base-loopback-angular-admin",
  "version": "1.0.0",
  "main": "server/server.js",
  "angular-cli": {},
  "scripts": {
    "build:sdk": "./node_modules/.bin/lb-sdk server/server.js client/src/app/shared/sdk -i enabled",
    "lint": "eslint . && tslint \"client/src/**/*.ts\"",
    "start": "ng build && node .",
    "dev": "ng serve & node-dev .",
    "test": "ng test --single-run && ./node_modules/jasmine/bin/jasmine.js JASMINE_CONFIG_PATH=jasmine.json",
    "posttest.bak": "npm run lint && nsp check",
    "pree2e": "webdriver-manager update",
    "e2e": "(cd client && protractor)"
  },
  "dependencies": {
    "@angular/common": "2.4.8",
    "@angular/compiler": "2.4.8",
    "@angular/core": "2.4.8",
    "@angular/forms": "2.4.8",
    "@angular/http": "2.4.8",
    "@angular/platform-browser": "2.4.8",
    "@angular/platform-browser-dynamic": "2.4.8",
    "@angular/router": "3.4.8",
    "compression": "1.6.2",
    "cors": "2.8.1",
    "helmet": "3.5.0",
    "loopback": "3.5.0",
    "loopback-boot": "2.23.0",
    "loopback-component-explorer": "4.1.1",
    "loopback-connector-mysql": "3.0.0",
    "core-js": "^2.4.1",
    "rxjs": "5.2.0",
    "serve-favicon": "2.4.1",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.7.2",
    "strong-error-handler": "2.0.0",
    "zone.js": "^0.7.6"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0-rc.4",
    "@angular/compiler-cli": "^2.4.0",
    "@mean-expert/loopback-sdk-builder": "^2.1.0-rc.2",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "@types/socket.io-client": "^1.4.29",
    "codelyzer": "~2.0.0",
    "enhanced-resolve": "^2.3.0",
    "eslint": "3.18.0",
    "eslint-config-loopback": "8.0.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-coverage-istanbul-reporter": "^0.2.2",
    "node-dev": "^3.1.3",
    "nsp": "^2.1.0",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.4.2",
    "typescript": "~2.0.0",
    "webdriver-manager": "10.2.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CodifiLLC/base-loopback-angular-admin.git"
  },
  "license": "MIT",
  "description": "A base project (Angular 2, Loopback 3, and Socket.io) for building an application with user account and admin functions"
}
