{
  "name": "base-loopback-angular-admin",
  "version": "1.0.0",
  "main": "server/server.js",
  "angular-cli": {},
  "scripts": {
    "build:sdk": "./node_modules/.bin/lb-sdk server/server.js client/src/app/shared/sdk -i disabled -l angular2",
    "lint": "eslint . && tslint \"client/src/**/*.ts\" -e \"client/src/app/shared/sdk/**\"",
    "start": "ng build && node .",
    "dev": "ng serve & node-dev .",
    "test": "ng test --watch=false && ./node_modules/jasmine/bin/jasmine.js JASMINE_CONFIG_PATH=jasmine.json",
    "posttest.bak": "npm run lint && nsp check",
    "pree2e": "webdriver-manager update",
    "e2e": "(cd client && protractor)"
  },
  "dependencies": {
    "@angular/animations": "^6.1.6",
    "@angular/common": "^6.1.6",
    "@angular/compiler": "^6.1.6",
    "@angular/core": "^6.1.6",
    "@angular/forms": "^6.1.6",
    "@angular/http": "^6.1.6",
    "@angular/platform-browser": "^6.1.6",
    "@angular/platform-browser-dynamic": "^6.1.6",
    "@angular/router": "^6.1.6",
    "bootstrap": "^4.1.3",
    "compression": "1.6.2",
    "core-js": "^2.5.7",
    "cors": "^2.8.3",
    "font-awesome": "^4.7.0",
    "helmet": "3.5.0",
    "loopback": "^3.9.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^4.3.1",
    "loopback-connector-mysql": "^4.2.0",
    "ngx-bootstrap": "^3.0.1",
    "rxjs": "^6.3.2",
    "serve-favicon": "^2.5.0",
    "shared-git-hooks": "^1.2.1",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.7.2",
    "strong-error-handler": "^2.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.1",
    "@angular/cli": "^6.2.1",
    "@angular/compiler-cli": "^6.1.6",
    "@mean-expert/loopback-sdk-builder": "^2.3.1",
    "@types/jasmine": "2.8.6",
    "@types/node": "^10.9.4",
    "@types/socket.io-client": "^1.4.29",
    "codelyzer": "~2.0.0",
    "enhanced-resolve": "^3.4.1",
    "eslint": "^3.19.0",
    "eslint-config-loopback": "8.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.3",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-dev": "^3.1.3",
    "nsp": "^2.1.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "^5.11.0",
    "typescript": "~2.9.2",
    "webdriver-manager": "10.2.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CodifiLLC/base-loopback-angular-admin.git"
  },
  "license": "MIT",
  "description": "A base project (Angular 2, Loopback 3, and Socket.io) for building an application with user account and admin functions"
}
